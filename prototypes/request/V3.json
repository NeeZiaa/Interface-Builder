{
    "context": {
        "theme": "dark",
        "font": "roboto",
        "style": "inventory",
        "width": "100%",
        "height": "100%",
        "position": "top-left"
    },
    "components": {
        "header": {
            "props": {
                "title": "Hello c'est un exemple",
                "banner": "assets/image.png",
                "description": "Petit texte de description en bas de header",
            }, 
            "form": {
                "title": "Recherche",
                "description": "Recherchez un item",
                "button": {
                    "display": "Rechercher",
                    "callback": "search_item",
                    "style": ["dark", "rounded"]
                },
                "input": {
                    "placeholder": "Rechercher un item"
                },
                "style": ["light", "transparent"]
            }
        },
        "field": {
            "props": {
                "label": "Field",
                "icon": "Icon",
                "style": ["light", "transparent"],
                "callback": "field_callback"
            },
            "textfield": {
                "props": {
                    "name": "Name",
                    "placeholder": "Placeholder",
                    "value": "Value"
                }
            }
        },
        "footer": {
            "props": {
                "description": "Copyright &copy 2023...",
                "image": "assets/image_test.png"
            }
        }        
    }
}